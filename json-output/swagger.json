          }
        }
      },
      "EPCISDocument_EPCISBody_EventList": {
        "type": "object",
        "properties": {
          "ObjectEvent": {
            "$ref": "#/components/schemas/EPCISDocument_EPCISBody_EventList_ObjectEvent"
          }
        }
      },
      "EPCISDocument_EPCISBody": {
        "type": "object",
        "properties": {
